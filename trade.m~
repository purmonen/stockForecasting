function money = trade(realPrices, predictedPrices, transactionFee, sellLimit)
predictedCash = 1;
isIn = false;
for i=2:length(predictedPrices)
    trans = 0;
    if ~isIn
        trans = predictedCash * 0.12 / 100 * 1;
    end
    if predictedPrices(i) >= allRealPrices(i-1) + trans
        predictedCash = (predictedCash-trans) * allRealPrices(i) / allRealPrices(i-1);
        isIn = true;
    elseif isIn && allPredictedPrices(i) >= allRealPrices(i-1) * 0.99
        predictedCash = (predictedCash) * allRealPrices(i) / allRealPrices(i-1);
    else
        isIn = false;
    end
end
money = predictedCash
end